<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.washington.ruiheli.tictactoe">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity android:name=".SignUp" />
        <activity android:name=".MainMenu" />
        <activity android:name=".OnlineSetting" />
        <activity android:name=".OfflineSetting" />
        <activity android:name=".OnlineGameBoard" />
        <activity android:name=".OfflineGameBoard" />
        <activity android:name=".Rooms" />
        <activity android:name=".HostWaiting" />
        <activity android:name=".Stats" />
    </application>

</manifest>